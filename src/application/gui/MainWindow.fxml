                                          <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" HBox.hgrow="ALWAYS">
                                             <children>
                                                <TextField fx:id="allProductsFilter" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" promptText="Filter Factory Products" />
                                                <ListView fx:id="allProductsList" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
                                             </children>
                                          </VBox>
                                          <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0">
                                             <children>
                                                <AnchorPane>
                                                   <children>
                                                      <Button fx:id="addFactoryProduct" mnemonicParsing="false" onAction="#onAddFactoryProductPressed" text="&gt;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" GridPane.columnIndex="1" GridPane.hgrow="NEVER" GridPane.rowIndex="1" />
                                                   </children>
                                                </AnchorPane>
                                                <AnchorPane>
                                                   <children>
                                                      <Button fx:id="removeFactoryProduct" mnemonicParsing="false" onAction="#onRemoveFactoryProductPressed" text="&lt;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" GridPane.columnIndex="1" GridPane.hgrow="NEVER" GridPane.rowIndex="2" />
                                                   </children>
                                                </AnchorPane>
                                             </children>
                                          </VBox>
                                          <TableView fx:id="factoryProductsTable" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                                             <columns>
                                                <TableColumn fx:id="factoryProductsTableProductNameColumn" editable="false" text="Item Name" />
                                                <TableColumn fx:id="factoryProductsTableProductionRateColumn" text="Count" />
                                                <TableColumn fx:id="factoryProductsTableProductionRateUnitColumn" text="Per" />
                                             </columns>
                                             <columnResizePolicy>
                                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                             </columnResizePolicy>
                                          </TableView>
                                       </children>
                                    </HBox>
                                 </children>
                              </AnchorPane>
                           </content>
                        </TitledPane>
                        <TitledPane collapsible="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Select Factory Inputs" VBox.vgrow="SOMETIMES">
