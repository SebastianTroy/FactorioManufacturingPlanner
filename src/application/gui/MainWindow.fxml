              <MenuItem fx:id="loadRecipiesButton" mnemonicParsing="false" onAction="#onLoadRecipiesButoonClicked" text="Load Recipies" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
                  <RadioMenuItem fx:id="defaultCssButton" mnemonicParsing="false" onAction="#onDefaultCssClicked" text="Default">
                     <toggleGroup>
                        <ToggleGroup fx:id="cssGroup" />
                     </toggleGroup>
                  </RadioMenuItem>
                  <RadioMenuItem fx:id="darkCssButton" mnemonicParsing="false" onAction="#onDarkCssClicked" text="Dark" toggleGroup="$cssGroup" />
