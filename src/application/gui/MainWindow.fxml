                                          <TableColumn editable="false" prefWidth="75.0" text="Column X" />
                                      </columns>
                                       <columnResizePolicy>
                                          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                       </columnResizePolicy>
                                    </TableView>
                                 </children>
                              </AnchorPane>
                           </content>
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </TitledPane>
                     </children>
                  </VBox>
               </content>
          </TitledPane>
        </panes>
      </Accordion>
   </children>
</VBox>
